	level_sprites = {}
	build_level()
	
	player = {
		x = 63,
		y = 120,
		direction=0, -- 0 = left, 1 = up, 2 = right, 3 = down --
		base_spr_index = 0,
		current_animation=nil,
		c = 3,
		drawcolor = 11,
		dead = false,
